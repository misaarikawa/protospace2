.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(prototype.prototype_images.main[0].content), prototype_path(prototype)
    .caption
      %h3
      = prototype.title
      .proto-meta
        .proto-user
          = prototype.user.name
        .proto-posted
          = prototype.created_at.strftime("%m月%d日")
    %ul.proto-tag-list.list-inline
      %li
        %a{href: "#", class: "btn btn-default"} iPad
      %li
        %a{href: "#", class: "btn btn-default"} wireframe
